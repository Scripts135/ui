	ZIndex = 4
})

local userId = LocalPlayer.UserId
local thumbType = Enum.ThumbnailType.HeadShot
local thumbSize = Enum.ThumbnailSize.Size48x48

local function updatePlayerInfo()
	if Library.HideInfo then
		local randomDecalId = nil
		local decalFound = false
		
		for _, obj in pairs(workspace:GetDescendants()) do
			if obj:IsA("Decal") and obj.Texture ~= "" then
				randomDecalId = obj.Texture
				decalFound = true
				break
			end
		end
		
		if decalFound and randomDecalId then
			PlayerIcon.Image = randomDecalId
		else
			warn("no decal found")
			PlayerIcon.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"
		end
		
		if PlayerIcon:FindFirstChild("Blur") then
			PlayerIcon.Blur:Destroy()
		end
	else
		pcall(function()
			local content = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
			PlayerIcon.Image = content
		end)
		if PlayerIcon:FindFirstChild("Blur") then
			PlayerIcon.Blur:Destroy()
		end
	end
end

updatePlayerInfo()

createInstance("UICorner", {
	Parent = PlayerIcon,
	CornerRadius = UDim.new(1, 0)
})

