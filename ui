-- // edit only below if you have a combine iq of 12
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

function createInstance(className, properties)
	local instance = Instance.new(className)
	for k, v in pairs(properties) do
		if typeof(k) ~= 'string' then
			continue
		end
		instance[k] = v
	end
	return instance
end

local function makeDraggable(frame, dragHandle)
	local dragging = false
	local dragInput, startPos, framePos

	dragHandle = dragHandle or frame

	dragHandle.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			startPos = input.Position
			framePos = frame.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	dragHandle.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			local delta = input.Position - startPos
			frame.Position = UDim2.new(
				framePos.X.Scale,
				framePos.X.Offset + delta.X,
				framePos.Y.Scale,
				framePos.Y.Offset + delta.Y
			)
		end
	end)
end

local function createTween(object, goal, duration)
	duration = duration or 0.3
	local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	local tween = TweenService:Create(object, tweenInfo, goal)
	return tween
end

local Library = {}
Library.Tabs = {}
Library.CurrentTab = nil
Library.NotificationPosition = "BottomRight"
Library.HideInfo = false
Library.ToggleKey = Enum.KeyCode.RightShift
Library.ConfigFolder = "NeptuneConfigs"
Library.ConfigData = {}
Library.ConfigCallbacks = {}

-- // if they dont have a file system ban them from the discord server 
local hasFileSystem = false
pcall(function()
	if isfolder and makefolder and writefile and readfile and delfile and listfiles then
		hasFileSystem = true
	end
end)

-- // conffig system (yes i had to write this down cause i was crashing out)
function Library:SaveConfig(configName)
	if not hasFileSystem then
		self:Notify({
			Type = "Error",
			Title = "Config Error",
			Description = "Your executor doesn't support file system",
			Duration = 3
		})
		return false
	end
	
	if not configName or configName == "" then
		self:Notify({
			Type = "Error",
			Title = "Config Error",
			Description = "Please enter a config name",
			Duration = 3
		})
		return false
	end
	
	local success = pcall(function()
		if not isfolder(self.ConfigFolder) then
			makefolder(self.ConfigFolder)
		end
		local data = game:GetService("HttpService"):JSONEncode(self.ConfigData)
		writefile(self.ConfigFolder .. "/" .. configName .. ".neptune", data)
	end)
	
	if success then
		self:Notify({
			Type = "Success",
			Title = "Config Saved",
			Description = "Config '" .. configName .. "' saved successfully",
			Duration = 2
		})
		return true
	else
		self:Notify({
			Type = "Error",
			Title = "Config Error",
			Description = "Failed to save config",
			Duration = 3
		})
		return false
	end
end

function Library:LoadConfig(configName)
	if not hasFileSystem then
		self:Notify({
			Type = "Error",
			Title = "Config Error",
			Description = "Your executor doesn't support file system",
			Duration = 3
		})
		return false
	end
	
	if not configName or configName == "" or configName == "No configs" then
		self:Notify({
			Type = "Error",
			Title = "Config Error",
			Description = "Please select a config to load",
			Duration = 3
		})
		return false
	end
	
	local success = pcall(function()
		local data = readfile(self.ConfigFolder .. "/" .. configName .. ".neptune")
		local decoded = game:GetService("HttpService"):JSONDecode(data)
		
		for key, value in pairs(decoded) do
			if self.ConfigCallbacks[key] then
				self.ConfigCallbacks[key](value, true)
			end
			self.ConfigData[key] = value
		end
	end)
	
	if success then
		self:Notify({
			Type = "Success",
			Title = "Config Loaded",
			Description = "Config '" .. configName .. "' loaded successfully",
			Duration = 2
		})
		return true
	else
		self:Notify({
			Type = "Error",
			Title = "Config Error",
			Description = "Failed to load config",
			Duration = 3
		})
		return false
	end
end

function Library:DeleteConfig(configName)
	if not hasFileSystem then
		self:Notify({
			Type = "Error",
			Title = "Config Error",
			Description = "Your executor doesn't support file system",
			Duration = 3
		})
		return false
	end
	
	if not configName or configName == "" or configName == "No configs" then
		self:Notify({
			Type = "Error",
			Title = "Config Error",
			Description = "Please select a config to delete",
			Duration = 3
		})
		return false
	end
	
	local success = pcall(function()
		delfile(self.ConfigFolder .. "/" .. configName .. ".neptune")
	end)
	
	if success then
		self:Notify({
			Type = "Success",
			Title = "Config Deleted",
			Description = "Config '" .. configName .. "' deleted successfully",
			Duration = 2
		})
		return true
	else
		self:Notify({
			Type = "Error",
			Title = "Config Error",
			Description = "Failed to delete config",
			Duration = 3
		})
		return false
	end
end

function Library:GetConfigs()
	if not hasFileSystem then
		return {}
	end
	
	local success, result = pcall(function()
		if not isfolder(self.ConfigFolder) then
			makefolder(self.ConfigFolder)
			return {}
		end
		
		local configs = {}
		local files = listfiles(self.ConfigFolder)
		for _, file in ipairs(files) do
			local name = file:gsub(self.ConfigFolder .. "\\", ""):gsub(self.ConfigFolder .. "/", ""):gsub(".neptune", "")
			table.insert(configs, name)
		end
		return configs
	end)
	
	if success and result then
		return result
	else
		return {}
	end
end

function Library:AutoLoadConfig()
	if not hasFileSystem then
		return false
	end
	
	local configs = self:GetConfigs()
	if #configs > 0 then
		self:LoadConfig(configs[1])
		return true
	end
	return false
end

function Library:SetConfigValue(key, value, callback)
	self.ConfigData[key] = value
	if callback then
		self.ConfigCallbacks[key] = callback
	end
end

